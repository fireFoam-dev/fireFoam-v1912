FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

//internalField   uniform 101325;
internalField   uniform 0;

boundaryField
{
    ground
    {
        type            fixedFluxPressure;
        value           $internalField;
    }
    outlet
    {
        type            prghTotalHydrostaticPressure;
        p0              $internalField;
        value           $internalField;
    }

    burner
    {
        type            fixedFluxPressure;
        value           $internalField;
    }

    region0_to_fuelRegion_fuel
    {
        type            fixedFluxPressure;
        value           $internalField;
    }
}
